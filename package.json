{
  "name": "front-end-starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "percy": "percy exec -- npm run cy:run",
    "start": "webpack-dev-server --config webpack.js",
    "start:serve": "serve -s ./dist",
    "build": "webpack --config webpack.js --mode production --devtool false",
    "lint:js": "eslint src/ --fix",
    "lint:css": "stylelint --fix src/**/*.css",
    "precommit": "lint-staged",
    "sonar": "node sonar-project.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tensorflow-models/body-pix": "^2.0.5",
    "@tensorflow/tfjs": "^1.7.2",
    "@tensorflow/tfjs-backend-wasm": "^1.7.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@percy/cypress": "^2.3.1",
    "@types/cypress": "^1.1.3",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "canvas": "^2.6.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "cypress-file-upload": "^4.0.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "imagemin-webpack-plugin": "^2.4.2",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.10",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "serve": "^11.3.0",
    "sonarqube-scanner": "^2.6.0",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/setup-jest.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setup-tests.js"
    ],
    "coverageReporters": [
      "html",
      "clover"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.history",
      "<rootDir>/cypress"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "browserslist": [
    "last 2 chrome versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "pre-push": "npm test"
    }
  }
}
